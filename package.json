{
  "name": "@galexc/gatsby-plugin-posthog",
  "version": "1.1.0",
  "description": "Temporary fork of official gatsby plugin to setup posthog that supports gatsby v5",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@testing-library/react": "^11.2.2",
    "babel-preset-gatsby-package": "^0.7.0",
    "cross-env": "^7.0.2",
    "gatsby": ">=2.0.0",
    "react": ">=16.4.2"
  },
  "peerDependencies": {
    "gatsby": ">=2.0.0",
    "react": ">=16.4.2",
    "react-dom": ">=16.4.2"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "posthog-analytics"
  ],
  "author": "GalexC",
  "license": "MIT"
}
